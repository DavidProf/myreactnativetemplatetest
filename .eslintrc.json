{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["plugin:react/recommended", "standard", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "@typescript-eslint"],
    "rules": {
        "indent": ["error", 4],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "valid-jsdoc": "error"
    }
}
